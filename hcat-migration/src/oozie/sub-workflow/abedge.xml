<workflow-app name="ab-subwf" xmlns="uri:oozie:workflow:0.2">
   <start to="fork-move"/> 
   <fork name="fork-move">
   	  <path start="appointment-move"/>
      <path start="servicerecord-move"/>
      <!-- 
      <path start="media-move"/>
      <path start="patient-spatial-move"/>     
      <path start="service-move"/>
      <path start="hca-move"/>
      -->
   </fork>
   <join name="joining-move" to="ab-subwf-end"/>

    <action name="appointment-move">
         <sub-workflow>
             <app-path>${abwf}/appointment.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>
            
         </sub-workflow>	
        <ok to="joining-move" />
         <error to="kill" />
    </action>
 
    <action name="servicerecord-move">
	 	<sub-workflow>
             <app-path>${abwf}/servicerecord.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>

         </sub-workflow>
         <ok to="joining-move" />
         <error to="kill" />
    </action> 
    
    <action name="media-move">
	 <sub-workflow>
             <app-path>${abwf}/media.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>

         </sub-workflow>
         <ok to="joining-move" />
         <error to="kill" />
    </action>     
  
    <action name="patient-spatial-move">
         <sub-workflow>
             <app-path>${abwf}/patient-spatial.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>
            
         </sub-workflow>	
        <ok to="joining-move" />
         <error to="kill" />
    </action>
	
    <action name="service-move">
	 	<sub-workflow>
             <app-path>${abwf}/service.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>

         </sub-workflow>
         <ok to="joining-move" />
         <error to="kill" />
    </action>
    
    <action name="hca-move">
	 <sub-workflow>
             <app-path>${abwf}/hca.xml</app-path>
             <propagate-configuration />
            <configuration>
                <property>
                    <name>jobTracker</name>
                    <value>${jobTracker}</value>
                </property>
                <property>
                    <name>nameNode</name>
                    <value>${nameNode}</value>
                </property>
                <property>
                    <name>queueName</name>
                    <value>${queueName}</value>
                </property>
            </configuration>

         </sub-workflow>
         <ok to="joining-move" />
         <error to="kill" />
    </action>    
	
 
  <kill name="kill">
        <message>Script failed, error message[${wf:errorMessage(wf:lastErrorNode())}]</message>
  </kill> 

  
   <end name="ab-subwf-end"/>

 </workflow-app>
